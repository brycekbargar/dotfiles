# shellcheck shell=bash
# shellcheck disable=SC2168
# vi: ft=sh

if [[ -z $BW_SESSION ]]; then
	echo >&2 "Vault is locked, run bw-unlock"
	return 1
fi

bw list items |
	jq -r \
		'
	.[] |
	[	.folderId // "/",
		.name, .login.uris[0].uri // "",
		.login.username // "",
		.id
	] |
	@tsv
	' |
	fzf --with-nth 1..-2 -d '\t' |
	awk '{print $NF}'
